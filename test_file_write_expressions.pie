        // Test 5: Expression with multiple concatenations
        float pi = 3.14159;
        file_write(f, "Value of PI: " + pi + "\n");
        
        // Test 6: Complex expression
        int x = 10;
        int y = 20;
        file_write(f, "Sum: " + (x + y) + "\n");
        
