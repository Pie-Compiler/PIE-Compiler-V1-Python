int fibonacci(int n) {
    if (n <= 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    } else {
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

void printFibonacci(int n) {
    int i = 0;
    while (i <= n) {
        int result = fibonacci(i);
        output(result, int);
        output(" ", string);
        i = i + 1;
    }
    // output("\n", string);
}

int n = 10;
printFibonacci(n);
